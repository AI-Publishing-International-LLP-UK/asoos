# Diamond SAO Command Center Environment Configuration
# Full system access with 8500+ Integration Gateway instances

# Core System Configuration
NODE_ENV=development
DIAMOND_SAO_ACCESS=true
COMMAND_CENTER_LEVEL=DIAMOND_SAO
ACCESS_LEVEL=UNLIMITED

# Console API Server Configuration  
CONSOLE_API_PORT=3001
CONSOLE_WS_URL=ws://localhost:3001
CONSOLE_API_URL=http://localhost:3001

# Integration Gateway Configuration
INTEGRATION_GATEWAY_COUNT=8500
MAX_CONCURRENT_CONNECTIONS=50000
INTEGRATION_GATEWAY_BASE_URL=http://localhost:3000

# Testament Swarm Configuration
TESTAMENT_SWARM_ENABLED=true
TOTAL_WINGS=13
SETTLEMENT_SWARMS=7
TOTAL_AGENTS=160000000
CIVILIZATION_AI_ENABLED=true

# Database Configuration (for production deployment)
# DATABASE_URL=postgresql://username:password@localhost:5432/asoos_diamond_sao
# REDIS_URL=redis://localhost:6379
# MONGODB_URI=mongodb://localhost:27017/asoos_command_center

# Security Configuration
SESSION_SECRET=diamond-sao-command-center-secret-key
JWT_SECRET=diamond-sao-jwt-secret-key
ENCRYPTION_KEY=diamond-sao-encryption-key

# External Service URLs
ACADEMY_PORT=8080
ACADEMY_URL=http://localhost:8080
MCP_BASE_URL=https://mcp.asoos.2100.cool

# Cloud Configuration (for production)
# GOOGLE_CLOUD_PROJECT=api-for-warp-drive
# GOOGLE_CLOUD_REGION=us-west1
# GCP_SECRET_MANAGER_ENABLED=false

# Monitoring and Logging
LOG_LEVEL=info
METRICS_ENABLED=true
REAL_TIME_MONITORING=true
HEALTH_CHECK_INTERVAL=30000

# Revenue and Business Metrics
ARR_TARGET=847000000
GROWTH_RATE_TARGET=2340
ENTERPRISE_CLIENTS=8500
DIAMOND_SAO_CLIENTS=47

# Features Flags
WEBSOCKET_ENABLED=true
REAL_TIME_UPDATES=true
COMMAND_EXECUTION=true
MULTI_TENANT_SUPPORT=true
AUTO_SCALING=true

# Performance Configuration
MAX_WEBSOCKET_CONNECTIONS=5000
API_RATE_LIMIT=10000
REQUEST_TIMEOUT=30000
KEEPALIVE_TIMEOUT=65000

# Development Configuration
HOT_RELOAD=true
DEBUG_MODE=true
VERBOSE_LOGGING=true
MOCK_EXTERNAL_SERVICES=false
