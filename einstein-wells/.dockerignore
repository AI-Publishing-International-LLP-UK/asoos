# Standard Node.js ignores
node_modules
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Architecture-specific binaries and files (ARM64/Darwin exclusion)
**/node_modules/**/vendor/**/darwin-*
**/node_modules/**/bin/darwin-*
**/node_modules/**/*-darwin-*
**/node_modules/**/*arm64*
**/*.dylib
**/*.bundle
**/prebuilds/darwin-*
**/prebuilds/osx-*

# Sharp-specific ARM64 binaries
**/node_modules/@img/sharp-darwin-arm64/**
**/node_modules/@img/sharp-libvips-darwin-arm64/**

# Cloudflare-specific ARM64 binaries
**/node_modules/@cloudflare/workerd-darwin-arm64/**

# fsevents (Mac-specific)
**/node_modules/fsevents/**

# Git
.git/
.gitignore

# Development files
*.env*

# Logs and build artifacts
logs/
*.log
dist/
build/
*.tgz

# OS generated files
.DS_Store
.DS_Store?
._*
.Trashes
Thumbs.db
