  "item": [
    {
      "name": "Authentication",
      "description": "SallyPort security center authentication endpoints",
      "item": [
        {
          "name": "SallyPort Token Verification",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{sallyport_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{sallyport_token}}\",\n  \"action\": \"verify\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/sallyport/verify",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "auth",
                "sallyport",
                "verify"
              ]
            }
          },
          "response": []
        },
        {
          "name": "OAuth2 Authentication",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"grant_type\": \"authorization_code\",\n  \"code\": \"{{auth_code}}\",\n  \"client_id\": \"{{client_id}}\",\n  \"client_secret\": \"{{client_secret}}\",\n  \"redirect_uri\": \"{{redirect_uri}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/oauth2/token",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "oauth2",
                "token"
              ]
            }
          },
          "response": []
        }
      ]
    },
