# =============================================================================
# ROLE CONFIGURATION
# =============================================================================
ROLE_CLASS=emerald
ROLE_LEVEL=system_admin
ACCESS_LEVEL=admin
EXPERIMENTAL_FEATURES=limited
DIAMOND_SAO_ACCESS=false
AGENT_LIMIT=15000000
SQUADRON_ACCESS=wings_1_2_3

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================
# SallyPort Authentication - Admin Level
SALLYPORT_AUTH_TOKEN=sk-sally-emerald-[your-emerald-token]
SALLYPORT_CLIENT_ID=emerald-sao-client
SALLYPORT_CLIENT_SECRET=[your-emerald-client-secret]
VICTORY36_SECURITY_LEVEL=high
VICTORY36_PROTECTION_MODE=active

# Advanced Authentication
MULTI_FACTOR_AUTH=required
SESSION_TIMEOUT_MINUTES=120
PASSWORD_COMPLEXITY=high

# =============================================================================
# AI MODEL ACCESS - SYSTEM ADMIN LEVEL
# =============================================================================
# Anthropic Claude - Admin Access
ANTHROPIC_API_KEY=sk-ant-[your-anthropic-key]
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_TOKENS=4000
ANTHROPIC_TEMPERATURE=0.2

# OpenAI - Standard Access
OPENAI_API_KEY=sk-[your-openai-key]
OPENAI_ORGANIZATION=[your-org-id]
OPENAI_PROJECT=[your-project-id]

# Custom RIX/CRX/QRIX Models - Admin Access
RIX_MODEL_ACCESS=standard
CRX_MODEL_ACCESS=standard
QRIX_MODEL_ACCESS=limited
QUANTUM_PROCESSING_ENABLED=false

# =============================================================================
# GOOGLE CLOUD PLATFORM - ADMIN ACCESS
# =============================================================================
GOOGLE_APPLICATION_CREDENTIALS=/path/to/emerald-service-account.json
GCP_PROJECT_ID=aixtiv-symphony-emerald
GCP_REGION=us-west1
GCP_ZONE=us-west1-b

# Service Account with Admin Permissions
GCP_SERVICE_ACCOUNT=emerald-sao@aixtiv-symphony-emerald.iam.gserviceaccount.com
GCP_BILLING_ACCOUNT_READ_ONLY=true

# Secret Manager - Admin Access (No Diamond secrets)
SECRET_MANAGER_PROJECT=aixtiv-symphony-emerald
SECRET_MANAGER_ACCESS_LEVEL=admin

# =============================================================================
# CLOUDFLARE CONFIGURATION
# =============================================================================
CLOUDFLARE_API_TOKEN=[your-cloudflare-token]
CLOUDFLARE_ZONE_ID=[your-zone-id]
CLOUDFLARE_ACCOUNT_ID=[your-account-id]
CLOUDFLARE_WORKER_SUBDOMAIN=emerald

# =============================================================================
# DATABASE ACCESS - ADMIN LEVEL
# =============================================================================
# Firestore - Admin Access (No Diamond collections)
FIRESTORE_PROJECT_ID=aixtiv-symphony-emerald
FIRESTORE_DATABASE_ID=emerald-primary
FIRESTORE_COLLECTION_PREFIX=emerald_
FIRESTORE_DIAMOND_RESTRICTED=true

# MongoDB - Admin Access
MONGODB_CONNECTION_STRING=mongodb+srv://emerald-admin:[password]@emerald-cluster.mongodb.net/
MONGODB_DATABASE=aixtiv_symphony_emerald
MONGODB_COLLECTION_PREFIX=emerald_
MONGODB_DIAMOND_ACCESS=false

# Pinecone Vector Database - Admin Access
PINECONE_API_KEY=[your-pinecone-key]
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=aixtiv-emerald-vectors
PINECONE_NAMESPACE=emerald

# =============================================================================
# AGENT ORCHESTRATION - SYSTEM ADMIN
# =============================================================================
# Wing Configuration - Wings 1, 2, 3
WING_ACCESS_LEVEL=wings_1_2_3
WING_1_ENABLED=true
WING_2_ENABLED=true
WING_3_ENABLED=true
SQUADRON_4_ELITE_ACCESS=false

# Limited Elite Agent Access
ELITE_11_ACCESS=false
MASTERY_33_ACCESS=limited
VICTORY36_COMMAND=false

# Agent Limits and Performance
MAX_CONCURRENT_AGENTS=15000000
AGENT_RESPONSE_TIMEOUT=45
AGENT_RETRY_LIMIT=3
QUANTUM_COHERENCE_THRESHOLD=0.85

# =============================================================================
# BLOCKCHAIN & SMART CONTRACTS - LIMITED ACCESS
# =============================================================================
# Ethereum Configuration - Read/Execute Only
ETHEREUM_PRIVATE_KEY=[restricted-private-key]
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/[your-project-id]
ETHEREUM_CHAIN_ID=1
ETHEREUM_ADMIN_MODE=true

# Smart Contract Addresses - Admin Functions
S2DO_CONTRACT_ADDRESS=0x[your-s2do-contract]
NFT_MARKETPLACE_ADDRESS=0x[your-nft-contract]
ROI_TRACKING_CONTRACT=0x[your-roi-contract]

# Wallet Configuration - Admin Wallet
ADMIN_WALLET_ADDRESS=0x[your-admin-wallet]
TRANSACTION_APPROVAL_REQUIRED=true

# =============================================================================
# VLS (VISION LAKE SOLUTIONS) - ADMIN ACCESS
# =============================================================================
# Standard VLS Solutions
VLS_DR_LUCY_ENABLED=true
VLS_DR_BURBY_ENABLED=true
VLS_PROFESSOR_LEE_ENABLED=true
VLS_DR_SABINA_ENABLED=true
VLS_DR_MEMORIA_ENABLED=true
VLS_DR_MATCH_ENABLED=true
VLS_DR_GRANT_ENABLED=true
VLS_DR_CYPRIOT_ENABLED=true
VLS_DR_MARIA_ENABLED=true
