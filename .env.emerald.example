# Emergency Access
EMERGENCY_CONTACT=[admin-emergency@aixtiv.com]
EMERGENCY_ESCALATION=diamond_sao_required
EMERGENCY_ACTIONS=limited

# Backup and Recovery - Admin Level
BACKUP_ACCESS=read_write
RECOVERY_EXECUTION=supervised
DISASTER_RECOVERY=coordinated

