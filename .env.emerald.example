# VLS Configuration - Admin Level
VLS_ACCESS_LEVEL=admin
VLS_EXPERIMENTAL_FEATURES=disabled
VLS_CUSTOM_DOMAINS=limited

# =============================================================================
# MONITORING & ANALYTICS - SYSTEM ADMIN
# =============================================================================
# Application Performance Monitoring
APM_ENABLED=true
APM_SERVICE_NAME=aixtiv-symphony-emerald
APM_ENVIRONMENT=production
APM_SAMPLE_RATE=0.8

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=structured
LOG_DESTINATION=centralized
AUDIT_LOGGING=standard

# Metrics Collection
METRICS_ENABLED=true
METRICS_ENDPOINT=/metrics
CUSTOM_METRICS=limited
BUSINESS_INTELLIGENCE=standard

# =============================================================================
# INTEGRATION SERVICES
# =============================================================================
# MCP (Model Context Protocol) Server
MCP_SERVER_URL=https://emerald-mcp.aixtiv.com
MCP_CLIENT_ID=emerald-sao-mcp
MCP_CLIENT_SECRET=[your-mcp-secret]
MCP_QUANTUM_FEATURES=disabled

# External Integrations - Admin Level
XERO_API_KEY=[your-xero-admin-key]
STRIPE_SECRET_KEY=sk_live_[your-stripe-admin-key]
STRIPE_PUBLISHABLE_KEY=pk_live_[your-stripe-admin-publishable]

# Communication Services - Admin Level
SENDGRID_API_KEY=[your-sendgrid-admin-key]
TWILIO_ACCOUNT_SID=[your-twilio-admin-sid]
TWILIO_AUTH_TOKEN=[your-twilio-admin-token]

# =============================================================================
# DEVELOPMENT & DEPLOYMENT
# =============================================================================
# Environment Settings
NODE_ENV=production
DEBUG=aixtiv:emerald:*
PORT=8080
HOST=0.0.0.0

# Build Configuration
BUILD_OPTIMIZATION=standard
COMPRESSION_ENABLED=true
CACHE_STRATEGY=standard

# Deployment Settings
DEPLOYMENT_REGION=us-west1
BACKUP_REGION=eu-west1
AUTO_SCALING=limited
LOAD_BALANCING=standard

# =============================================================================
# FEATURE FLAGS - EMERALD ADMIN
# =============================================================================
