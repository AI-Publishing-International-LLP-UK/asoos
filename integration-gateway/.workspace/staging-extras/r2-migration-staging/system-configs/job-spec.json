{
  "worker_pool_specs": [
    {
      "machine_spec": {
        "machine_type": "n1-standard-4"
      },
      "replica_count": 1,
      "container_spec": {
        "image_uri": "gcr.io/deeplearning-platform-release/tf2-cpu.2-8",
        "command": ["python3", "-m", "lucy_auto.main"],
        "args": [
          "--task",
          "code-review",
          "--data-path",
          "gs://api-for-warp-drive-data/input/",
          "--output-path",
          "gs://api-for-warp-drive-data/output/",
          "--log-path",
          "gs://api-for-warp-drive-data/logs/"
        ]
      }
    }
  ]
}
