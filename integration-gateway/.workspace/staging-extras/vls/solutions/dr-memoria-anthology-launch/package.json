{
  "name": "dr-memoria-anthology",
  "version": "1.0.1",
  "description": "Automated Publishing Solution for AIXTIV Symphony",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc && node scripts/build.js",
    "build:watch": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "dev": "nodemon src/index.js",
    "start": "node dist/index.js",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:youtube": "node dist/scripts/deploy-youtube.js",
    "deploy:linkedin": "node dist/scripts/deploy-linkedin.js",
    "generate-docs": "typedoc"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vision": "^4.0.2",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "jose": "^5.1.3",
    "linkedin-api-client": "^1.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.24.1",
    "web3": "^4.3.0",
    "ethers": "^6.9.0",
    "ipfs-http-client": "^60.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aixtiv-symphony/dr-memoria-anthology.git"
  },
  "keywords": [
    "aixtiv",
    "symphony",
    "anthology",
    "publishing",
    "memoria",
    "youtube",
    "linkedin",
    "blockchain",
    "nft",
    "content"
  ],
  "author": "AIXTIV Symphony Team",
  "license": "UNLICENSED",
  "private": true
}
