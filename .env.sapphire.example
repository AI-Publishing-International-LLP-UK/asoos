VLS_DR_SABINA_ENABLED=true
VLS_DR_MEMORIA_ENABLED=true
VLS_DR_MATCH_ENABLED=true
VLS_DR_GRANT_ENABLED=false
VLS_DR_CYPRIOT_ENABLED=true
VLS_DR_MARIA_ENABLED=true
VLS_DR_ROARK_ENABLED=false
VLS_DR_CLAUDE_ENABLED=false

# VLS Configuration - Business Level
VLS_ACCESS_LEVEL=business
VLS_EXPERIMENTAL_FEATURES=disabled
VLS_CUSTOM_DOMAINS=disabled

# =============================================================================
# MONITORING & ANALYTICS - ORGANIZATION FOCUS
# =============================================================================
# Application Performance Monitoring
APM_ENABLED=true
APM_SERVICE_NAME=aixtiv-symphony-sapphire
APM_ENVIRONMENT=production
APM_SAMPLE_RATE=0.5

# Logging Configuration
LOG_LEVEL=warn
LOG_FORMAT=structured
LOG_DESTINATION=organization
