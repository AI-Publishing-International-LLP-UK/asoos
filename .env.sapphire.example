# =============================================================================

# =============================================================================
# ROLE CONFIGURATION
# =============================================================================
ROLE_CLASS=sapphire
ROLE_LEVEL=organization_admin
ACCESS_LEVEL=organization
EXPERIMENTAL_FEATURES=disabled
DIAMOND_SAO_ACCESS=false
AGENT_LIMIT=10000000
SQUADRON_ACCESS=wings_1_2

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================
# SallyPort Authentication - Organization Level
SALLYPORT_AUTH_TOKEN=sk-sally-sapphire-[your-sapphire-token]
SALLYPORT_CLIENT_ID=sapphire-sao-client
SALLYPORT_CLIENT_SECRET=[your-sapphire-client-secret]
VICTORY36_SECURITY_LEVEL=standard
VICTORY36_PROTECTION_MODE=standard

# Organization Authentication
ORGANIZATION_ID=[your-organization-id]
MULTI_FACTOR_AUTH=required
SESSION_TIMEOUT_MINUTES=90
PASSWORD_COMPLEXITY=standard

# =============================================================================
# AI MODEL ACCESS - ORGANIZATION LEVEL
# =============================================================================
# Anthropic Claude - Standard Access
ANTHROPIC_API_KEY=sk-ant-[your-anthropic-key]
ANTHROPIC_MODEL=claude-3-haiku-20240307
ANTHROPIC_MAX_TOKENS=3000
ANTHROPIC_TEMPERATURE=0.3

# OpenAI - Basic Access
OPENAI_API_KEY=sk-[your-openai-key]
OPENAI_ORGANIZATION=[your-org-id]
OPENAI_PROJECT=[your-project-id]

# Custom RIX/CRX/QRIX Models - Limited Access
RIX_MODEL_ACCESS=basic
CRX_MODEL_ACCESS=standard
QRIX_MODEL_ACCESS=disabled
QUANTUM_PROCESSING_ENABLED=false

# =============================================================================
# GOOGLE CLOUD PLATFORM - ORGANIZATION ACCESS
# =============================================================================
GOOGLE_APPLICATION_CREDENTIALS=/path/to/sapphire-service-account.json
GCP_PROJECT_ID=aixtiv-symphony-sapphire
GCP_REGION=us-west1
GCP_ZONE=us-west1-b

# Service Account with Organization Permissions
GCP_SERVICE_ACCOUNT=sapphire-sao@aixtiv-symphony-sapphire.iam.gserviceaccount.com
GCP_BILLING_ACCOUNT_READ_ONLY=true

# Secret Manager - Organization Secrets Only
SECRET_MANAGER_PROJECT=aixtiv-symphony-sapphire
SECRET_MANAGER_ACCESS_LEVEL=organization

# =============================================================================
# CLOUDFLARE CONFIGURATION
# =============================================================================
CLOUDFLARE_API_TOKEN=[your-cloudflare-token]
CLOUDFLARE_ZONE_ID=[your-zone-id]
CLOUDFLARE_ACCOUNT_ID=[your-account-id]
CLOUDFLARE_WORKER_SUBDOMAIN=sapphire

# =============================================================================
# DATABASE ACCESS - ORGANIZATION LEVEL
# =============================================================================
# Firestore - Organization Collections Only
FIRESTORE_PROJECT_ID=aixtiv-symphony-sapphire
FIRESTORE_DATABASE_ID=sapphire-org
FIRESTORE_COLLECTION_PREFIX=sapphire_
FIRESTORE_ORGANIZATION_SCOPE=true

# MongoDB - Organization Access
MONGODB_CONNECTION_STRING=mongodb+srv://sapphire-org:[password]@sapphire-cluster.mongodb.net/
MONGODB_DATABASE=aixtiv_symphony_sapphire
MONGODB_COLLECTION_PREFIX=sapphire_
MONGODB_ORGANIZATION_ID=[your-org-id]

# Pinecone Vector Database - Organization Namespace
PINECONE_API_KEY=[your-pinecone-key]
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=aixtiv-sapphire-vectors
PINECONE_NAMESPACE=sapphire_org

# =============================================================================
# AGENT ORCHESTRATION - ORGANIZATION LEVEL
# =============================================================================
# Wing Configuration - Wings 1, 2 Only
WING_ACCESS_LEVEL=wings_1_2
WING_1_ENABLED=true
WING_2_ENABLED=true
WING_3_ENABLED=false
SQUADRON_4_ELITE_ACCESS=false

# Standard Agent Access
ELITE_11_ACCESS=false
MASTERY_33_ACCESS=false
VICTORY36_COMMAND=false

# Agent Limits and Performance
MAX_CONCURRENT_AGENTS=10000000
AGENT_RESPONSE_TIMEOUT=60
AGENT_RETRY_LIMIT=3
QUANTUM_COHERENCE_THRESHOLD=0.75

# =============================================================================
# LICENSING & SUBSCRIPTION MANAGEMENT
# =============================================================================
# License Management
LICENSE_MANAGEMENT_ENABLED=true
SUBSCRIPTION_TRACKING=enabled
RENEWAL_AUTOMATION=true
USAGE_MONITORING=comprehensive

# Client Management
CLIENT_PORTAL_ENABLED=true
CLIENT_BILLING_ACCESS=true
CLIENT_SUPPORT_TIER=standard
CLIENT_ONBOARDING=automated

# =============================================================================
# BLOCKCHAIN & SMART CONTRACTS - READ ONLY
# =============================================================================
# Ethereum Configuration - Read Only
ETHEREUM_PRIVATE_KEY=[read-only-key]
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/[your-project-id]
ETHEREUM_CHAIN_ID=1
ETHEREUM_READ_ONLY=true

# Smart Contract Addresses - Query Only
S2DO_CONTRACT_ADDRESS=0x[your-s2do-contract]
NFT_MARKETPLACE_ADDRESS=0x[your-nft-contract]
ROI_TRACKING_CONTRACT=0x[your-roi-contract]

# Organization Wallet - View Only
ORGANIZATION_WALLET_ADDRESS=0x[your-org-wallet]
