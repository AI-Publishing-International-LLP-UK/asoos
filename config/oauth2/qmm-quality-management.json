  "scopes": [
    "quality:read",
    "quality:write",
    "compliance:read",
    "compliance:write",
    "metrics:read",
    "metrics:write",
    "audit:read",
    "audit:write",
    "reporting:read",
    "reporting:write",
    "admin:manage"
  ],
  
  "endpoints": {
    "apiBase": "https://api.qmm.2100.cool/v1",
    "quality": "https://api.qmm.2100.cool/v1/quality",
    "compliance": "https://api.qmm.2100.cool/v1/compliance",
    "metrics": "https://api.qmm.2100.cool/v1/metrics",
