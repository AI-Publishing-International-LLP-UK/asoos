# ASOOS 2100.cool Landing Page Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to git

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_PROJECT_ID=your-project-id-here

# Anthropic Claude Configuration
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
CLAUDE_API_KEY=sk-ant-your-claude-key-here

# GitHub Configuration
GITHUB_TOKEN=ghp_your-github-token-here
GITHUB_APP_ID=your-github-app-id
GITHUB_PRIVATE_KEY=your-github-private-key

# Google Cloud Configuration
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com

# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
CLOUDFLARE_ZONE_ID=your-zone-id
CLOUDFLARE_ACCOUNT_ID=your-account-id

# Atlassian Configuration
ATLASSIAN_ACCESS_TOKEN=your-atlassian-token

# LangChain Configuration
LANGCHAIN_API_KEY=your-langchain-key

# Database Configuration
DATABASE_URL=your-database-connection-string

# Application Settings
NODE_ENV=development
PORT=3000
BASE_URL=https://2100.cool

# Security Settings
JWT_SECRET=your-jwt-secret-key-here
SESSION_SECRET=your-session-secret-key

# ASOOS Specific Configuration
ASOOS_ENVIRONMENT=development
DIAMOND_SAO_ACCESS_KEY=your-diamond-sao-key
INTEGRATION_GATEWAY_URL=your-integration-gateway-url
