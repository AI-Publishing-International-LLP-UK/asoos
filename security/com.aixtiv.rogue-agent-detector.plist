<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.aixtiv.rogue-agent-detector</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/node</string>
        <string>/Users/as/asoos/aixtiv-symphony/security/rogue-agent-detector.js</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/as/asoos/aixtiv-symphony</string>
    
    <key>StandardOutPath</key>
    <string>/Users/as/asoos/aixtiv-symphony/security/detector.log</string>
    
