        <key>CLOUD_ML_REGION</key>
        <string>us-west1</string>
    </dict>
    
    <key>ProcessType</key>
    <string>Background</string>
    
    <key>LowPriorityBackgroundIO</key>
    <false/>
    
    <key>Nice</key>
    <integer>-10</integer>
</dict>
</plist>