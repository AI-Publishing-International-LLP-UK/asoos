    "local-deploy": "./scripts/deploy-local.sh",
    "lint": "eslint *.js --max-warnings 0",
    "lint:fix": "eslint *.js --fix --max-warnings 0",
    "format": "prettier --write *.js",
    "format:check": "prettier --check *.js"
  },
